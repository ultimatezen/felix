<?xml version="1.0" encoding="utf-8"?>
<root>
  <head>
    <language>EN</language>
  </head>
  <records>
    <record>
      <id>lookup</id>
      <text>&amp;Lookup  (Alt+L)</text>
      <tooltip>Looks up the current cell.</tooltip>
    </record>
    <record>
      <id>lookup_next</id>
      <text>Look up Ne&amp;xt  (Alt+R ARR)</text>
      <tooltip>Selects and looks up the next cell.</tooltip>
    </record>
    <record>
      <id>set</id>
      <text>S&amp;et  (Alt+UP ARR)</text>
      <tooltip>Registers the translation for the current lookup cell.</tooltip>
    </record>
    <record>
      <id>set_and_next</id>
      <text>&amp;Set And Next  (Alt+S)</text>
      <tooltip>Registers the translation for the current cell, and look up the next one.</tooltip>
    </record>
    <record>
      <id>get</id>
      <text>Ge&amp;t  (Alt+DWN ARR)</text>
      <tooltip>Retrieves the translation for the current cell.</tooltip>
    </record>
    <record>
      <id>get_and_next</id>
      <text>&amp;Get And Next  (Alt+G)</text>
      <tooltip>Retrieves the translation for the current cell, and look up the next one.</tooltip>
    </record>
    <record>
      <id>next</id>
      <text>&amp;Next  (Alt+N)</text>
      <tooltip>Displays the next translation in the Felix window.</tooltip>
    </record>
    <record>
      <id>prev</id>
      <text>&amp;Previous  (Alt+P)</text>
      <tooltip>Displays the previous translation in the Felix window.</tooltip>
    </record>
    <record>
      <id>add_gloss</id>
      <text>Add Glossa&amp;ry</text>
      <tooltip>Batch adds glossary entries to the Felix glossary window.</tooltip>
    </record>
    <record>
      <id>add_memory</id>
      <text>Add &amp;Memory</text>
      <tooltip>Batch adds memory entries to the Felix memory window.</tooltip>
    </record>
    <record>
      <id>switch_lang</id>
      <text>S&amp;witch Menu to Japanese</text>
      <tooltip>Changes the menu and message language to Japanese.</tooltip>
    </record>

    <record>
      <id>entry</id>
      <text>Entry </text>
      <tooltip>Entry </tooltip>
    </record>

    <record>
      <id>gloss</id>
      <text>Glossa&amp;ry</text>
      <tooltip>Retrieves a glossary entry.</tooltip>
    </record>
    
    <record>
      <id>auto_trans</id>
      <text>&amp;Auto Translate...</text>
      <tooltip>Auto translation</tooltip>
    </record>
    
    <record>
      <id>auto_trans_sel</id>
      <text>&amp;Selection</text>
      <tooltip>Automatically translate all cells in selection.</tooltip>
    </record>
    
    <record>
      <id>auto_trans_sheet</id>
      <text>S&amp;heet</text>
      <tooltip>Automatically translate the current sheet</tooltip>
    </record>
    
    <record>
      <id>auto_trans_all</id>
      <text>All &amp;Workbooks</text>
      <tooltip>Automatically translate all sheets in all open workbooks</tooltip>
    </record>
    
  </records>

</root>